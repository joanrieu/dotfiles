"systemctl --user exit"
  Alt + Shift + E

"termite"
  Alt + Return

"tmux list-sessions | dmenu | egrep -o ^[0-9]+ | xargs echo tmux attach-session -t | xargs -0 termite -e"
  Alt + Shift + Return

"dmenu_run"
  Alt + D

"mpc toggle"
  XF86AudioPlay

"mpc stop"
  Control + XF86AudioPlay

"mpc prev"
  XF86AudioPrev

"mpc next"
  XF86AudioNext

"amixer set Surround toggle"
  XF86AudioMute

"amixer set Surround off"
  Control + XF86AudioMute

"amixer set Surround 4%+"
  XF86AudioRaiseVolume

"amixer set Surround 4%-"
  XF86AudioLowerVolume
