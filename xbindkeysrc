"systemctl --user exit"
  Alt + Shift + E

"roxterm"
  Alt + Return

"session="$(tmux list-sessions | dmenu | awk -F: '{ print $1 }')" && [[ ! -z "$session" ]] && roxterm -e tmux attach-session -t "$session""
  Alt + Shift + Return

"dmenu_run"
  Alt + D

"mpc toggle"
  XF86AudioPlay

"mpc stop"
  Control + XF86AudioPlay

"mpc prev"
  XF86AudioPrev

"mpc next"
  XF86AudioNext

"amixer set Surround toggle"
  XF86AudioMute

"amixer set Surround off"
  Control + XF86AudioMute

"amixer set Surround 4%+"
  XF86AudioRaiseVolume

"amixer set Surround 4%-"
  XF86AudioLowerVolume
